# Example environment file - Copy this to .env.development and fill in your credentials
# DO NOT commit actual credentials to version control

# Client-side environment variables (accessible via import.meta.env)
VITE_GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID
VITE_GOOGLE_REDIRECT_URI=YOUR_REDIRECT_URI

# Server-side environment variables (for Netlify functions)
GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID
GOOGLE_CLIENT_SECRET=YOUR_GOOGLE_CLIENT_SECRET
GOOGLE_REDIRECT_URI=YOUR_REDIRECT_URI
